{
    "rules": {
        "no-unused-vars": [2, {"vars": "local", "args": "after-used"}],
        "indent": [2, 4],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "space-infix-ops": [2, {"int32Hint": false}],
        "eqeqeq": [2, "smart"]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "ROT": true,
        "TT": true
    },
    "extends": "eslint:recommended"
}
